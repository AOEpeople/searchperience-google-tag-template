const sendPixel = require('sendPixel');
const setCookie = require('setCookie');
const getCookieValues = require('getCookieValues');
const localStorage = require('localStorage');
const copyFromDataLayer = require('copyFromDataLayer');
const getQueryParameters = require('getQueryParameters');
const getType = require('getType');
const encodeUriComponent = require('encodeUriComponent');
const getUrl = require('getUrl');
const logToConsole = require('logToConsole');

const log = data.debug ? logToConsole : (() => { });

const maybeCookies = function (cookieName, uuid) {
  let cookies;
  cookies = getCookieValues(cookieName);
  if (cookies.length === 0) {
    const options = {
      'domain': getUrl('host'),
      'path': '/',
      'secure': true,
      'samesite': true
    };
    cookies = uuid;
    setCookie(cookieName, cookies, options);
  }
  return cookies;
};

const getSession = function() {
  let session = maybeCookies('spSession', data.spUserSession);
  if(getType(session) === "array") {
    return session[0];
  }
  return session;
};

let usession = getSession();
log("usession: " + usession);

const maybeLocalStorage = function(key, value) {
	if (localStorage) {
		let itemValue;
		// Requires read access for the key. Returns null if the key does not exist.
		itemValue = localStorage.getItem(key); 
		if(itemValue === null) { 
    		// Requires write access for the key. Returns true if successful.
			let wasSavedToLocalStorage = localStorage.setItem(key, value);
 			if (wasSavedToLocalStorage === false) {
        		log("Could not be saved to localStorage");
	        }
        }
      	return itemValue;
    }
	return maybeCookies(key, value);
};

let uidentifier = maybeLocalStorage('spUser', data.spUserCookie);
log("uidentifier: " + uidentifier);

const searchQuery = getQueryParameters('q');
log("searchQuery: " + searchQuery);

const spTrackerUrl = data.spTrackerUrl;
log("spTrackerUrl: " + spTrackerUrl);

const userId = copyFromDataLayer("userId");
log("userId: " + userId);

const transformObjToUrlParams = function (obj) {
  let urlParams = "";
  for (var key in obj) {
    if (urlParams != "") {
      urlParams += "&";
    }
    urlParams += key + "=" + encodeUriComponent(obj[key]);
  }
  return urlParams;
};

const buildTrackerRequestsParams = function (productData, action) {
  let spProducts = [];
  let spTrackerRequestParams = [];

  // objects from which we will build the URL for the sp-tracker
  let trackerObj = {
    ucookie: uidentifier,
    usession: usession,
    action: action
  };

  // add the user id, promenade sends per default "0" when user is not logged in
  // this would make data-processing believe that all interaction are done by user "0"
  if (userId !== '0') {
    trackerObj.uid = userId;
  }

  log('Found ' + productData.length + ' products');

  // based on dataLayer event we have one or many products 
  productData.forEach(product => {
    // build searchperience product
    let spProduct = {
      // build the foreign id
      foreignId: product.id + '-' + product.locale + '-' + product.channel,
      // transform brand to brand code, e.g. "JOHNNIE WALKER" > johnnie-walker
      brandCode: product.brand.toLowerCase().replace(' ', '-'),
      // transform category to category code, e.g. "Liquors/Spirits" > online_liquous_spirits
      categoryCode: product.channel + '_' + product.category.toLowerCase().replace('/', '_'),
      channel: product.channel,
      locale: product.locale
    };

    if (getType(product.variant) !== 'undefined') {
      spProduct.variantId = product.variant + '-' + product.locale + '-' + product.channel;
    }
    
    spProducts.push(spProduct);
  });

  if (spProducts.length > 0) {
    log('Generating tracker URL for action: ' + action);
    if (action === 'search') {
      log ('Generating search event tracker URL with query: ' + searchQuery);
      if (getType(searchQuery) !== 'undefined') {
        trackerObj.q = searchQuery;
        // join all products into a string for tracking search event
        trackerObj.results = spProducts.map(spProduct => spProduct.foreignId).join(',');
        trackerObj.channel = spProducts[0].channel;
        trackerObj.locale = spProducts[0].locale;
        spTrackerRequestParams.push(transformObjToUrlParams(trackerObj));
      }
    } else {
      spTrackerRequestParams = spProducts.map(spProduct => {
        // add common params for other actions like purchase, click, view, addToCart
        trackerObj.imaster = spProduct.foreignId;
        trackerObj.brandCode = spProduct.brandCode;
        trackerObj.categoryCodes = spProduct.categoryCode;
        trackerObj.channel = spProduct.channel;
        trackerObj.locale = spProduct.locale;
        // add param 'item' to actions like addToCart and purchase
        if ((action === 'addToCart' || action === 'purchase') && getType(spProduct.variantId) != 'undefined') {
          trackerObj.item = spProduct.variantId;
        }
        return transformObjToUrlParams(trackerObj);
      });
    }
  }

  return spTrackerRequestParams;

};

// this object allows to to map the google's enhanced ecommerce events 
// to the searchperience tracking events
let eventTypeToActionMapping = {
  'productClick': {
    action: "click",
    productDataPath: "ecommerce.click.products"
  },
  'productDetailImpression': {
    action: "view",
    productDataPath: "ecommerce.detail.products"
  },
  'addToCart': {
    action: "addToCart",
    productDataPath: "ecommerce.add.products"
  },
  'purchase': {
    action: "purchase",
    productDataPath: "ecommerce.purchase.products"
  },
  'productImpression': {
    action: "search",
    productDataPath: "ecommerce.impressions"
  }
};

let currentMapping = eventTypeToActionMapping[data.eventType];

if (typeof (currentMapping) === 'undefined') {
  log('Unable to map eventType to action: ' + data.eventType);
  data.gtmOnFailure();

} else {
  log('Using mapping for event: ' + data.eventType);

  let productData = copyFromDataLayer(currentMapping.productDataPath);
  log('Using product data: ' + productData.map(data => data.id).join(','));

  if (typeof (productData) === 'undefined' || productData.length == 0) {
    log('Unable to get product info from dataLayer');
    data.gtmOnFailure();

  } else {
    let trackerRequestsParams = buildTrackerRequestsParams(productData, currentMapping.action);
    log('Generated ' + trackerRequestsParams.length + ' tracker requests params');

    if (spTrackerUrl !== null) {
      trackerRequestsParams.forEach(trackerRequestsParam => {
        let completeTrackerUrl = spTrackerUrl + '_?' + trackerRequestsParam;
        log ('Generated request: ' + completeTrackerUrl);
        sendPixel(completeTrackerUrl);
      }, data.gtmOnSuccess);

    } else {
      log('No trackerUrl');
      data.gtmOnFailure();

    }
  }
}


